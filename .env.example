# Backend API URL
# For iOS Simulator, use localhost
# For physical device, use your computer's local IP (e.g., http://192.168.1.x:3000)
EXPO_PUBLIC_RORK_API_BASE_URL=http://localhost:3000

# Supabase Configuration
# Get these values from: https://supabase.com/dashboard/project/_/settings/api
EXPO_PUBLIC_SUPABASE_URL=your-project-url.supabase.co
EXPO_PUBLIC_SUPABASE_ANON_KEY=your-anon-key-here

# OneSignal Configuration
# Get your App ID from: https://onesignal.com/apps (Settings > Keys & IDs)
EXPO_PUBLIC_ONESIGNAL_APP_ID=your-onesignal-app-id-here
